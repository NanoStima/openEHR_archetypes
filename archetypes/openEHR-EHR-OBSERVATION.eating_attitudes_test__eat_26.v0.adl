	details = <
		["en"] = <
			language = <[ISO_639-1::en]>
			purpose = <"The purpose of eating attitude test (EAT-26) is to identify risk behavior for eating disorders. ">
			use = <"To be used in children, adolescents, and adults; 

To be used to identify risk behavior for eating disorders (e.g., aneoxia; food restriction); 

To be used in the self-fill form;

To be used  to english language population. This questionnaire was validated in Canada population;

To be used to Asian population. The screening has a good validity. ">
			keywords = <"EAT-26", "Eating disorders", "Eating attitudes", "Anorexia">
			misuse = <"Not to be used to eating disorder diagnosis. 

Not to be used for all population (e.g. Brazilian, Japonese, Spanish population must be used the EAT-26 scale validated for them). ">
			copyright = <"">
		>
	>
	lifecycle_state = <"0">
	other_contributors = <"Ricardo Cruz-Correia, MEDCIDS, FMUP; Virtual Care - Portugal ", "Duarte Ferreira, MEDCIDS; FMUP - Portugal ", "Pedro Vieira-Marques, MEDCIDS, FMUP - Portugal ", "Marle Alvarenga, USP - Brasil ">
	other_details = <
